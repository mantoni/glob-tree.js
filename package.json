{
  "name"             : "glob-tree",
  "version"          : "0.4.2",
  "description"      : "Tree with glob iterator",
  "keywords"         : ["tree", "iterator", "glob", "match"],
  "author"           : "Maximilian Antoni (http://maxantoni.de)",
  "homepage"         : "https://github.com/mantoni/glob-tree.js",
  "main"             : "./lib/node.js",
  "engines"          : {
    "node"           : ">=0.10"
  },
  "scripts"          : {
    "start"          : "mochify --watch --node --cover",
    "lint"           : "jslint --color './**/*.js'",
    "test-wd"        : "mochify --wd",
    "test"           : "npm run lint && mocha -R spec && mochify --cover"
  },
  "repository"       : {
    "type"           : "git",
    "url"            : "https://github.com/mantoni/glob-tree.js.git"
  },
  "dependencies"     : {
    "live-tree"      : "~0.3.0",
    "min-iterator"   : "~0.1.1",
    "inherits"       : "~2.0.1"
  },
  "devDependencies"  : {
    "mocha"          : ">=1.17 <2",
    "jslint"         : "~0.3.1",
    "mochify"        : "~0.6.0"
  },
  "files"            : ["lib", "README.md", "CHANGES.md", "LICENSE"],
  "license"          : "MIT"
}
